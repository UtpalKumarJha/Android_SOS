<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/plain"
    android:shrinkColumns="*"
    android:stretchColumns="*">
    <TableRow
        android:id="@+id/tableRow6"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center_horizontal">
        <TextView
            android:id="@+id/t1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="30dp"
            android:shadowDx="2"
            android:shadowDy="2"
            android:shadowRadius="1"
            android:shadowColor="#000000"
            android:text="ADD"
            android:padding="2dp"
            android:gravity="center_horizontal"
            android:layout_span="2"></TextView>
    </TableRow>
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center_horizontal"
        >
        <TextView
            android:id="@+id/t2"
            android:text="NUMBER"
            android:textStyle="bold"
            android:typeface="serif"
            android:gravity="center_horizontal"
            android:textSize="30dp"
            android:padding="2dp"
            android:layout_height="50dp">
        </TextView>
        <TextView
            android:id="@+id/t3"
            android:text="NAME"
            android:textStyle="bold"
            android:typeface="serif"
            android:gravity="center_horizontal"
            android:padding="2dp"
            android:textSize="30dp"
            android:layout_height="50dp"
            ></TextView>
    </TableRow>
    <TableRow
        android:layout_height="wrap_content"
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:gravity="center_horizontal">
        <TextView
            android:text=" "
            android:id="@+id/i1"
            android:textStyle="bold"
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:layout_height="60dp">
        </TextView>
        <TextView
            android:id="@+id/t4"
            android:text=""
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:textStyle="bold"
            android:padding="3dp"
            android:layout_height="60dp">
        </TextView>
    </TableRow>
    
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center_horizontal">
        <TextView
            android:id="@+id/i2"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_height="60dp">
        </TextView>
        <TextView
            android:id="@+id/t5"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_height="60dp">
        </TextView>
    </TableRow>
    
    <TableRow
        android:layout_height="wrap_content"
        android:id="@+id/tableRow4"
        android:layout_width="match_parent">
        <TextView
            android:text=""
            android:id="@+id/i3"
            android:padding="3dp"
            android:textStyle="bold"
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:layout_height="60dp"
            >
        </TextView>
        <TextView
            android:id="@+id/t6"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_height="60dp"></TextView>
    </TableRow>
    <TableRow
        android:id="@+id/tableRow5"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center">
        <TextView
            android:id="@+id/i4"
            android:text=""
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:padding="3dp"
            android:textStyle="bold"
            android:layout_height="60dp">
        </TextView>
        <TextView
            android:id="@+id/t7"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textStyle="bold"
            android:layout_height="60dp"
            android:textSize="20dp"
            >
        </TextView>
    </TableRow>
    <TableRow
        android:id="@+id/tableRow7"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center">
        <TextView
            android:id="@+id/i5"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_height="60dp">
        </TextView>
        <TextView
            android:id="@+id/t8"
            android:text=""
            android:padding="3dp"
            android:gravity="center_horizontal"
            android:textStyle="bold"
            android:layout_height="60dp"
            android:textSize="20dp">
        </TextView>
    </TableRow>
    
    <Button
        android:id="@+id/imageButton1"
        android:layout_width="20dp"
        android:layout_height="75dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:text="5" 
        android:textSize="30dp" 
        android:textStyle="bold" 
        android:background="@drawable/round_button"
        />
    
</TableLayout>


